<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagCloudView">
        <attr name="tcvBackground" format="reference" />
        <attr name="tcvTextColor" format="color" />
        <attr name="tcvBorder" format="dimension" />
        <attr name="tcvTextSize" format="dimension" />
        <attr name="tcvBorderItem" format="dimension" />
        <attr name="tcvItemBorderVertical" format="dimension" />
        <attr name="tcvItemBorderHorizontal" format="dimension" />
        <attr name="tcvSingleLine" format="boolean" />
        <attr name="tcvShowEndText" format="boolean" />
        <attr name="tcvShowRightImg" format="boolean" />
        <attr name="tcvEndText" format="string" />
        <attr name="tcvRightResId" format="reference" />
        <attr name="tcvCanTagClick" format="boolean" />
        <attr name="tcvTagResId" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomSlidingTabIndicator">
        <attr name="STIindicatorColor" format="color" />
        <attr name="STIunderlineColor" format="color" />
        <attr name="STIdividerColor" format="color" />
        <attr name="STItextColor" format="color" />
        <attr name="STIindicatorHeight" format="dimension" />
        <attr name="STIunderlineHeight" format="dimension" />
        <attr name="STIdividersPadding" format="dimension" />
        <attr name="STItabLeftRightPadding" format="dimension" />
        <attr name="STIscrollOffSet" format="dimension" />
        <attr name="STItabTextSize" format="dimension" />
        <attr name="STItabBackground" format="reference" />
        <attr name="STIshouldExpand" format="boolean" />
        <attr name="STItextCaps" format="boolean" />
        <attr name="STItabTopBtmPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SlidingTabLayout">
        <attr name="indicatorColor" format="color" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="tabBackgroundColor" format="reference" />
        <attr name="tabBackgroundTextViewId" format="reference" />
    </declare-styleable>

    <declare-styleable name="ContainerView">
        <attr name="card_item_height" format="dimension" />
        <attr name="card_item_margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference" />
        <attr name="rotation_degrees" format="float" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>

    <declare-styleable name="EnterLayoutAnimSupportContainer">
        <attr name="isAdjustResize" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SoundWaveView">
        <attr name="waveColor" format="color" />
        <attr name="soundOriginOrientation">
            <enum name="left" value="1" />
            <enum name="right" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MaterialProgressBarSupport">
        <attr name="progressSmallSize" format="boolean" />
        <attr name="progressColor" format="color" />
        <attr name="progressBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SimpleTagImageView">
        <attr name="simple_corner_distance" format="dimension" />
        <attr name="simple_tag_width" format="dimension" />
        <attr name="simple_tag_background_color" format="color" />
        <attr name="simple_tag_text" format="string" />
        <attr name="simple_tag_textSize" format="dimension" />
        <attr name="simple_tag_textColor" format="color" />
        <attr name="simple_tag_orientation" format="enum">
            <enum name="left_top" value="0" />
            <enum name="right_top" value="1" />
            <enum name="left_bottom" value="2" />
            <enum name="right_bottom" value="3" />
        </attr>
        <attr name="simple_tag_enable" format="boolean" />
        <attr name="simple_tag_round_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PullListView">
        <attr name="getMoreType">
            <enum name="auto" value="0" />
            <enum name="click" value="1" />
        </attr>
        <attr name="addPullHeaderByUser" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PullToZoomView">
        <attr name="headerView" format="reference" />
        <attr name="contentView" format="reference" />
        <attr name="zoomView" format="reference" />
        <attr name="isHeaderParallax" format="boolean" />
    </declare-styleable>

</resources>